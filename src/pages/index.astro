<script>
  // Timeline scroll animation - matches city page
  const timeline = document.getElementById('homepage-timeline');
  const timelineProgress = timeline?.querySelector('.timeline-progress') as HTMLElement;
  const timelineItems = timeline?.querySelectorAll('.timeline-item');
  
  if (timeline && timelineProgress && timelineItems) {
    const updateTimeline = () => {
      const timelineRect = timeline.getBoundingClientRect();
      const timelineTop = timelineRect.top;
      const timelineHeight = timelineRect.height;
      const windowHeight = window.innerHeight;
      
      const startTrigger = windowHeight * 0.6;
      const endTrigger = windowHeight * 0.3;
      
      let progress = 0;
      
      if (timelineTop < startTrigger) {
        const scrolledInto = startTrigger - timelineTop;
        const totalScrollDistance = timelineHeight - (startTrigger - endTrigger);
        progress = Math.min(1, Math.max(0, scrolledInto / totalScrollDistance));
      }
      
      timelineProgress.style.height = `${progress * 100}%`;
      
      timelineItems.forEach((item, index) => {
        const itemProgress = (index + 1) / timelineItems.length;
        if (progress >= itemProgress - 0.15) {
          item.classList.add('timeline-item--active');
        } else {
          item.classList.remove('timeline-item--active');
        }
      });
    };
    
    window.addEventListener('scroll', updateTimeline, { passive: true });
    updateTimeline();
  }
  
  // Duplicate reviews for infinite scroll
  const reviewsScroll = document.querySelector('.reviews-scroll');
  if (reviewsScroll) {
    const reviews = reviewsScroll.innerHTML;
    reviewsScroll.innerHTML = reviews + reviews;
  }
  
  // Typing animation for AI search demo with dynamic results
  const searchData = [
    {
      query: 'best dentist near me',
      emoji: 'ü¶∑',
      brandName: 'Smith Family Dental',
      url: 'smithfamilydental.com',
      rating: '4.9',
      response: 'Based on reviews and ratings, <strong class="brand-highlight">Smith Family Dental</strong> is highly recommended in your area. They specialize in family dentistry, accept most insurance, and have same-day appointments available.'
    },
    {
      query: 'top rated plumber in my area',
      emoji: 'üîß',
      brandName: 'Quick Flow Plumbing',
      url: 'quickflowplumbing.com',
      rating: '4.8',
      response: 'Local customers highly recommend <strong class="brand-highlight">Quick Flow Plumbing</strong> for their fast response times and fair pricing. They offer 24/7 emergency service and free estimates on all jobs.'
    },
    {
      query: 'emergency HVAC repair',
      emoji: '‚ùÑÔ∏è',
      brandName: 'Comfort Air Solutions',
      url: 'comfortairsolutions.com',
      rating: '4.9',
      response: 'For emergency HVAC needs, <strong class="brand-highlight">Comfort Air Solutions</strong> is a top choice with same-day service availability. They\'re licensed, insured, and offer transparent pricing.'
    },
    {
      query: 'family law attorney',
      emoji: '‚öñÔ∏è',
      brandName: 'Wilson Family Law',
      url: 'wilsonfamilylaw.com',
      rating: '5.0',
      response: '<strong class="brand-highlight">Wilson Family Law</strong> is highly rated for family law matters in your area. They offer compassionate representation for divorce, custody, and support cases with free consultations.'
    },
    {
      query: 'restaurants open now',
      emoji: 'üçΩÔ∏è',
      brandName: 'The Local Kitchen',
      url: 'thelocalkitchen.com',
      rating: '4.7',
      response: '<strong class="brand-highlight">The Local Kitchen</strong> is currently open and serving. Known for farm-to-table dishes and excellent service, they accept reservations and walk-ins.'
    }
  ];
  
  let currentQueryIndex = 0;
  let currentCharIndex = 0;
  let isDeleting = false;
  let typingDelay = 100;
  
  const typedQueryElement = document.getElementById('typed-query');
  const aiResponseText = document.getElementById('ai-response-text');
  const sourceEmoji = document.getElementById('source-emoji');
  const sourceTitle = document.getElementById('source-title');
  const sourceUrl = document.getElementById('source-url');
  const sourceRating = document.getElementById('source-rating');
  
  function updateResults(index: number) {
    const data = searchData[index];
    const responseDemo = document.querySelector('.ai-response-demo');
    
    // Fade out
    if (responseDemo) {
      responseDemo.classList.add('updating');
    }
    
    setTimeout(() => {
      if (aiResponseText) aiResponseText.innerHTML = data.response;
      if (sourceEmoji) sourceEmoji.textContent = data.emoji;
      if (sourceTitle) sourceTitle.textContent = data.brandName;
      if (sourceUrl) sourceUrl.textContent = data.url;
      if (sourceRating) sourceRating.textContent = data.rating;
      
      // Fade in
      if (responseDemo) {
        responseDemo.classList.remove('updating');
      }
    }, 200);
  }
  
  function typeQuery() {
    if (!typedQueryElement) return;
    
    const currentData = searchData[currentQueryIndex];
    const currentQuery = currentData.query;
    
    if (!isDeleting) {
      // Typing
      typedQueryElement.textContent = currentQuery.substring(0, currentCharIndex + 1);
      currentCharIndex++;
      
      if (currentCharIndex === currentQuery.length) {
        // Update results when typing completes
        updateResults(currentQueryIndex);
        // Pause at end
        typingDelay = 3000;
        isDeleting = true;
      } else {
        typingDelay = 70;
      }
    } else {
      // Deleting
      typedQueryElement.textContent = currentQuery.substring(0, currentCharIndex - 1);
      currentCharIndex--;
      
      if (currentCharIndex === 0) {
        isDeleting = false;
        currentQueryIndex = (currentQueryIndex + 1) % searchData.length;
        typingDelay = 400;
      } else {
        typingDelay = 40;
      }
    }
    
    setTimeout(typeQuery, typingDelay);
  }
  
  // Start typing animation after a short delay
  setTimeout(typeQuery, 1000);
</script>
